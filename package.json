{
  "name": "unplugin-vue-map-ui",
  "version": "0.0.6",
  "description": "Components resolver for auto importing",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/unplugin-vue-map-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/unplugin-vue-map-ui.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/unplugin-vue-map-ui/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{js,ts}": "prettier --write"
  },
  "scripts": {
    "build": "tsup && esno scripts/postbuild.ts",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:update": "vitest -u",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "dependencies": {
    "unplugin-vue-components": "^0.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-angular": "^17.6.1",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "del": "^7.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.1",
    "vitest": "^0.30.1",
    "vue": "^3.2.47"
  }
}
