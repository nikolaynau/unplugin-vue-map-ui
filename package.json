{
  "name": "unplugin-vue-map-ui",
  "version": "0.0.13",
  "description": "Components resolver for auto importing",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/unplugin-vue-map-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/unplugin-vue-map-ui.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/unplugin-vue-map-ui/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{js,ts}": "prettier --write"
  },
  "scripts": {
    "build": "tsup && esno scripts/postbuild.ts",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:update": "vitest -u",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "dependencies": {
    "unplugin-vue-components": "^0.25.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "del": "^7.1.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "esno": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "~5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vue": "^3.3.8"
  }
}
